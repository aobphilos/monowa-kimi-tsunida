{
  "name": "bot-api",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --repl --config moleculer.config.ts src/services/**/*.service.ts",
    "cli": "moleculer connect NATS",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "moleculer-repl": "^0.7.0",
    "ts-node": "^10.7.0"
  },
  "dependencies": {
    "@types/jest": "^27.4.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.21",
    "ioredis": "^4.28.5",
    "moleculer": "^0.14.19",
    "moleculer-web": "^0.10.4",
    "nats": "^2.6.1",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">= 14.x.x"
  },
  "workspaces": [
    "../../dist/libs/**"
  ]
}
